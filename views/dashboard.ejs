<!-- dashboard.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <h1>Welcome to Your Dashboard</h1>
    <h2>Past Luggage Records:</h2>
    <ul>
        <% luggageRecords.forEach(record => { %>
            <li>Desired Weight Limit: <%= record.desiredWeightLimit %>, Optimal Fee: <%= record.optimalFee %>, Timestamp: <%= record.timestamp %></li>
        <% }); %>
    </ul>
    <center><a href="/logout">Logout</a></center>
    <script src="/js/script.js"></script>
</body>
</html>
